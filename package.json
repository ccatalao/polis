{
  "name": "polis",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "https://ccatalao.github.io/polis",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "vite --config vite.config.js",
    "web:build": "vite build --config vite.config.js",
    "web:serve": "vite preview --config vite.config.js",
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "build:dev:android": "npx eas build --profile development --platform android",
    "build:dev:ios": "npx eas build --profile development --platform ios",
    "build:preview:android": "npx eas build --profile preview --platform android",
    "build:preview:ios": "npx eas build --profile preview --platform ios",
    "build:web": "node build-web.js",
    "predeploy": "npm run build:web",
    "deploy": "gh-pages -d web-build",
    "deploy:clean": "rm -rf web-build && npm run build:web && npm run deploy"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "axios": "^1.8.4",
    "expo": "^52.0.38",
    "expo-dev-client": "~3.3.12",
    "expo-image": "~1.10.5",
    "expo-status-bar": "~1.11.1",
    "leaflet": "^1.9.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.2.1",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "copy-webpack-plugin": "^13.0.0",
    "eas-cli": "^15.0.15",
    "gh-pages": "^6.1.1",
    "vite": "^5.0.10"
  },
  "private": true
}
