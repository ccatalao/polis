{
  "name": "polis",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "https://ccatalao.github.io/polis",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eas-build-pre-install": "npm config set legacy-peer-deps true",
    "build:dev:android": "npx eas build --profile development --platform android",
    "build:dev:ios": "npx eas build --profile development --platform ios",
    "build:preview:android": "npx eas build --profile preview --platform android",
    "build:preview:ios": "npx eas build --profile preview --platform ios",
    "clean:web-build": "rm -rf web-build/images",
    "predeploy": "rm -rf dist && mkdir -p dist && cp -r web-build/* dist/ && rm -rf dist/images && touch dist/.nojekyll",
    "deploy": "npm run clean:web-build && gh-pages -d dist"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "expo": "^52.0.38",
    "expo-dev-client": "~3.3.12",
    "expo-image": "~1.10.5",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "eas-cli": "^15.0.15",
    "gh-pages": "^6.1.1"
  },
  "private": true
}
